    "open-lavatory",
    "react-native"
  ],
  "homepage": "https://github.com/v3xlabs/open-lavatory",
  "bugs": {
    "url": "https://github.com/v3xlabs/open-lavatory/issues"
  },
  "repository": {
    "type": "git",
